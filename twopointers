#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int A[n],B[n];
	    for(int i=0;i<n;i++){
	        cin>>A[i];
	    }
	    for(int i=0;i<n;i++){
	        cin>>B[i];
	    }
	    int count=0;
	    for(int i=0;i<n;i++){
	        for(int j=0;j<n;j++){
	            if(A[i]==B[j]){
	                count++;
	                break;
	            }
	        }
	    }
	   cout<<count;
	}
	return 0;
}
